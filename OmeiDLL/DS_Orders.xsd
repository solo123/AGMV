          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="NotSeatsOrdersTableAdapter" GeneratorDataComponentClassName="NotSeatsOrdersTableAdapter" Name="NotSeatsOrders" UserDataComponentName="NotSeatsOrdersTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="OmeiTravelConnectionString (Settings)" DbObjectName="OmeiTravel.dbo.orders" DbObjectType="Table" GenerateMethods="Get" GenerateShortCommands="False" GeneratorGetMethodName="GetNotSeatedOrders" GetMethodModifier="Public" GetMethodName="GetNotSeatedOrders" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="False" UserGetMethodName="GetNotSeatedOrders" UserSourceName="GetNotSeatedOrders">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="True">
                    <CommandText>select
	orderId, firstName+'.'+lastName as username,
	numberAdult, numberChild, numberFree, contactPhone
from orders
where scheduleId=@scheduleId and status&lt;=3 and
   orderId not in
(
	select orderId
	from
	(
		select b.orderId, b.confSeat, orders.numberAdult+orders.numberChild+orders.numberFree as bookSeat
		from
			(
				select 
					orderId, count(*) as confSeat
				from BusSeats
				where 
					scheduleId=@scheduleId
				group by orderId
			) b
			left join orders on b.orderId=orders.orderId
	) seat
	where confSeat=bookSeat
)</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="False" AutogeneratedName="scheduleId" ColumnName="scheduleId" DataSourceName="OmeiTravel.dbo.orders" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@scheduleId" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="scheduleId" SourceColumnNullMapping="False" SourceVersion="Current">
                      </Parameter>
                    </Parameters>
                  </DbCommand>
                </SelectCommand>
